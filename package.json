  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e env/.env -- nodemon -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node index.js",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "kysely:init": "kysely init",
    "migrate:make": "kysely migrate:make",
    "migrate:latest": "dotenv -e env/.env -- kysely migrate:latest",
    "migrate:down": "dotenv -e env/.env -- kysely migrate:down",
    "migrate:up": "dotenv -e env/.env -- kysely migrate:up",
    "seed:make": "kysely seed:make -x=ts init-data",
    "seed:run": "dotenv -e env/.env -- kysely seed:run",
    "test": "jest"
  },
  "repository": {
    "type": "git",
